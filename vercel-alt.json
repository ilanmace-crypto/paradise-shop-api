{
  "version": 2,
  "functions": {
    "server/server.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server/server.js"
    },
    {
      "source": "/admin/(.*)",
      "destination": "/server/server.js"
    },
    {
      "source": "/health",
      "destination": "/server/server.js"
    }
  ]
}
