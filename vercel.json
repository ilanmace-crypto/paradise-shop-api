{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/vercel.js"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/server/vercel.js"
    },
    {
      "src": "/health",
      "dest": "/server/vercel.js"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/client/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/client/dist/index.html"
    }
  ]
}
